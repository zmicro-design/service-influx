export SERVICE_INFLUX_NAME=influx

export SERVICE_INFLUX_VERSION=1.8.4
export SERVICE_INFLUX_CONFIG=$SERVICE_CONFIG

export SERVICE_INFLUX_HOST=0.0.0.0
export SERVICE_INFLUX_PORT=8086

export SERVICE_INFLUX_ADMIN_USERNAME=
export SERVICE_INFLUX_ADMIN_PASSWORD=

export SERVICE_INFLUX_HTPASSWD_CONFIG=${SERVICE_CONFIGS}/htpasswd

if [ ! -f $SERVICE_INFLUX_CONFIG ]; then
    touch $SERVICE_INFLUX_CONFIG
fi

if [ ! -f $SERVICE_INFLUX_HTPASSWD_CONFIG ]; then
   cp $SERVICE_DIR/conf/htpasswd $SERVICE_INFLUX_HTPASSWD_CONFIG
fi